#!/user/bin/env python
#coding:utf-8
import threading
import time 

def Func(i):
    time.sleep(1)
    print i
    for item in range(10):
        print i,item


t=threading.Thread(target=Func,args=(1,))
t.start()
t.join()    #线程

t2 = threading.Thread(target=Func,args=(2,))
t2.start()
t2.join()


print 'OVER.'